
<div class="container">
<h1>Sign up</h1>
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= f.label :PseudoX %>
      <%= f.text_field :pseudo %>
      </br>
      <%= f.label :emailX %>
      <%= f.email_field :email %>
      </br>

      <%= f.label :passwordX %>
      <%= f.password_field :password %>
      </br>

      <%= f.label :password_confirmationX, "Confirmation" %>
      <%= f.password_field :password_confirmation %>
      </br>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
