<div class="container">
  <h2>Listing users</h2>
  <table class="table">
    <thead>
      <tr>
        <th>Pseudo</th>
        <th>Email</th>
        <th>Show</th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.pseudo %></td>
        <td><%= user.email %></td>
       <td><%= link_to 'Show', user_path(user) %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
